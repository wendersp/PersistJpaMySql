<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>usuarioCons</from-outcome>
            <to-view-id>/paginas/usuarioCons.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>usuarioFrm</from-outcome>
            <to-view-id>/paginas/usuarioFrm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>cidadeCons</from-outcome>
            <to-view-id>/paginas/cidadeCons.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>cidadeFrm</from-outcome>
            <to-view-id>/paginas/cidadeFrm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>estadoCons</from-outcome>
            <to-view-id>/paginas/estadoCons.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>estadoFrm</from-outcome>
            <to-view-id>/paginas/estadoFrm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>produtoCons</from-outcome>
            <to-view-id>/paginas/produtoCons.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>produtoFrm</from-outcome>
            <to-view-id>/paginas/produtoFrm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
    </navigation-rule>
    
</faces-config>
